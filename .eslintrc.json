{
  "extends": [
    "plugin:vue/vue3-essential",
    "@nuxt/eslint-config",
    "plugin:prettier/recommended",
    "plugin:storybook/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "rules": {
    "vue/no-multiple-template-root": "off",
    "vue/no-deprecated-slot-attribute": "off"
  }
}
